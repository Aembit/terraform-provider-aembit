provider "aembit" {
}

resource "aembit_credential_provider" "jwt_svid_token" {
	name = "TF Acceptance JWT-SVID Token - Modified"
	is_active = true
	jwt_svid_token = {
		subject = "spiffe://test.com/path"
		subject_type = "literal"
		lifetime_in_minutes = 60	
		audience = "test.aembit.io"
		algorithm_type = "RS256"
		custom_claims = [
			{
				key = "key"
				value = "value"
				value_type = "literal"
			},
			{
				key = "key2"
				value = "value2"
				value_type = "dynamic"
			}
		]
	}
}
